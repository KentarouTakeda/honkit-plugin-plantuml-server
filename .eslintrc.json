{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "settings": {
    "import/resolver": {
       "typescript": { "project": "./" }
    }
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:import/recommended",
    "plugin:import/warnings",
    "prettier"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
